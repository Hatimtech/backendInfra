extends layout

block content
  h1= title
  p Welcome to #{title}
  form(action= 'http://172.16.100.212:8080/OpenKM/services/rest/document/createSimple', method='POST', enctype="multipart/form-data")
    p
      | title:
      input(type='file', name='content', value='')
      |          docPath:
      input(type='text', name='docPath', value='/okm:root/newDoc.txt')

    input(type='submit', value='Submit')


